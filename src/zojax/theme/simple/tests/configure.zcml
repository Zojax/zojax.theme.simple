<configure
   xmlns="http://namespaces.zope.org/zope"
   xmlns:zojax="http://namespaces.zope.org/zojax">

  <interface
     name="DefaultSkin"
     interface=".tests.IDefaultSkin"
     type="zope.publisher.interfaces.browser.IBrowserSkinType" />

  <zojax:pagelet
     name="test.html"
     for="*"
     template="test.pt"
     permission="zope.Public" />

  <zojax:pagelet
     name="talesapi.html"
     for="*"
     template="talesapi.pt"
     permission="zope.Public" />

  <zojax:portlet
     name="portlet1"
     title="Portlet1"
     template="portlet1.pt" />

  <zojax:portlet
     name="portlet2"
     title="Portlet2"
     template="portlet2.pt" />

</configure>
